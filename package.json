{
  "name": "vlocityestools",
  "description": "Vlocity ES DevOps Tools",
  "version": "0.13.0",
  "author": "Juan Garcia",
  "bugs": "https://github.com/jfgarcia268/vlocity-es_devops_tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3.0.3",
    "@salesforce/core": "^2.12.3",
    "dir-compare": "2.3.0",
    "jsforce": "^1.10.0",
    "simple-git": "2.20.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "2.0.0",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^14",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^8",
    "nyc": "^15",
    "ts-node": "^9",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jfgarcia268/vlocity-es_devops_tools",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "vlocityestools": {
        "description": "VlocityeESTools distributes certain DevOps Utilities for a Salesforce/Vlocity Project. This tool is not an Official tool from Vlocity or Salesforce. Use it at your own risk."
      },
      "vlocityestools:clean": {
        "description": "Cleaning Commands"
      },
      "vlocityestools:compare": {
        "description": "Comparison Tools"
      },
      "vlocityestools:auth": {
        "description": "Auth Tools"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jfgarcia268/vlocity-es_devops_tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
